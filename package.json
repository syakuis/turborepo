{
  "name": "turborepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "eslint": "8.28.0",
    "msw": "1.3.2",
    "prettier": "2.8.0",
    "tsconfig": "*",
    "turbo": "latest",
    "typescript": "4.6.4"
  },
  "packageManager": "npm@10.1.0"
}
